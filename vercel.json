{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/callback",
            "dest": "api/index.py"
        },
        {
            "src": "/api/webhook",
            "dest": "api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "functions": {
        "api/index.py": {
            "maxDuration": 10
        }
    }
}